{
  "name": "vue2-project-work-template",
  "description": "基于 vue2 项目开发工程模板",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn dev",
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "prepare": "husky install"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^2.6.14",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@samhammer/vue-cli-plugin-stylelint": "^2.1.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.4",
    "css-properties-sorting": "^1.0.10",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "style-resources-loader": "^1.5.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-tencent": "^1.0.0",
    "stylelint-order": "^5.0.0",
    "vue-cli-plugin-style-resources-loader": "^0.1.5",
    "vue-template-compiler": "^2.6.14"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": "yarn lint",
    "src/**/*.{vue,js,scss,less,css,html}": "yarn lint:style"
  }
}
